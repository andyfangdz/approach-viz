{
  "name": "approach-viz",
  "version": "1.0.0",
  "description": "3D visualization for instrument approach procedures",
  "type": "module",
  "scripts": {
    "download-data": "bash scripts/download-data.sh",
    "build-db": "tsx scripts/build-db.ts",
    "prepare-data": "npm run download-data && npm run build-db",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:parser && npm run test:geometry",
    "test:parser": "node --import tsx --test lib/cifp/parser.test.ts",
    "test:geometry": "node --import tsx --test app/scene/approach-path/geometry.test.ts",
    "test:integration:runtime": "node --import tsx --test services/runtime-rs/runtime.integration.test.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "npm run prepare-data && node scripts/dev-with-ddtrace.mjs",
    "build": "npm run prepare-data && next build",
    "start": "next start"
  },
  "keywords": [
    "aviation",
    "instrument-approach",
    "cifp",
    "three.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@takram/three-geospatial": "^0.6.0",
    "@vercel/analytics": "^1.6.1",
    "3d-tiles-renderer": "^0.4.21",
    "better-sqlite3": "^11.8.1",
    "dd-trace": "^5.85.0",
    "egm96-universal": "^1.1.1",
    "kdbush": "^4.0.2",
    "next": "^16.1.6",
    "pdfjs-dist": "^4.10.38",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-select": "^5.10.2",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "eslint": "^9.39.1",
    "prettier": "^3.8.1",
    "tsx": "^4.20.0",
    "typescript-eslint": "^8.46.2",
    "typescript": "^5.9.3"
  }
}
